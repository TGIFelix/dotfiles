alias remoteoff="sudo systemsetup -setremotelogin off"
alias x="exit"
alias vimconfig="vim ${HOME}/.vimrc"
alias config="vim ${HOME}/.zshrc"
alias bs="brew search"
alias bi="brew install"
alias bci="brew cask install"
